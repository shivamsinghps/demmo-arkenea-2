{% extends '@Public/common/general-layout.html.twig' %}

{% block body_class %}{{ parent() }} transaction-history-page {% endblock body_class %}
{% block content %}
    <section class="content-header">
        <h1>{{ 'fmt.transaction_history.student.title'|trans }}</h1>
    </section>
    <section class="content">
        <div class="box box-primary">
            <div class="box-body">
                <div class="row">
                    <div class="col-md-12" data-ajax-container="container">
                        <div class="box box-success no-padding-lr">
                            {% include '@Public/student/transaction_history/_search.html.twig' %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
{% endblock content %}
{% block footer_scripts %}
    {% javascripts
        '@PublicBundle/Resources/public/js/handler/live-reaction.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock footer_scripts %}
